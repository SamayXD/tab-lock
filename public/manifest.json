{
  "manifest_version": 3,
  "name": "Hello World Extension",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.jpeg"
  },
 "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "scripting"
  ], "host_permissions": [
    "*://*.pixels.com/*"
  ],
  "description": "A simple Chrome extension built with React to display 'Hello, World!'.",
  "background": {
    "service_worker": "background.js"
  }
}