{
  "manifest_version": 3,
  "name": "Tab Lock Extension",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "scripting",
    "notifications",
    "webNavigation"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.jpeg"
  },
  "background": {
    "service_worker": "background.js"
  }
}